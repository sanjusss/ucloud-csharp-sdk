<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>UCloudSDK</AssemblyName>
    <Authors>icyflash</Authors>
    <Copyright>Copyright © UCloudSDK Project</Copyright>
    <Version>1.0.11.0</Version>
    <Product>UCloud API C# SDK</Product>
    <Description>UCloud C# SDK是用于调用UCloud API的类库，适用于.Net Core 2.0及以上。

UCloud API: http://docs.ucloud.cn/api/index.html</Description>
    <PackageLicenseUrl>https://raw.githubusercontent.com/sanjusss/ucloud-csharp-sdk/dotnetcore/License.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/sanjusss/ucloud-csharp-sdk</PackageProjectUrl>
    <RepositoryUrl>https://github.com/sanjusss/ucloud-csharp-sdk</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageReleaseNotes>UCloud C# SDK是用于调用UCloud API的类库，适用于.Net Core 2.0及以上。

UCloud API: http://docs.ucloud.cn/api/index.html</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>UCloudSDK.NetCore</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="106.3.1" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.5.0" />
  </ItemGroup>

</Project>
